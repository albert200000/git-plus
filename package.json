{
  "name": "pulsar-git-plus",
  "main": "./dist/main.js",
  "version": "8.7.1",
  "description": "Do git things without the terminal",
  "keywords": [
    "git"
  ],
  "providedServices": {
    "pulsar-git-plus": {
      "description": "Execute git commands",
      "versions": {
        "1.0.0": "provideService"
      }
    }
  },
  "scripts": {
    "test": "eslint --fix \"lib/**/*.js\" \"spec/**/*.js\" && apm test",
    "dev": "cross-env NODE_ENV=development parcel watch --target main lib/git-plus.js",
    "build": "cross-env NODE_ENV=production parcel build --target main lib/git-plus.js && cp lib/grammars/*.json dist/"
  },
  "deserializers": {
    "pulsar-git-plus/output-view": "deserializeOutputView"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "autosave": {
      "versions": {
        "1.0.0": "consumeAutosave"
      }
    },
    "tree-view": {
      "versions": {
        "1.0.0": "consumeTreeView"
      }
    }
  },
  "repository": "https://github.com/albert200000/git-plus",
  "license": "MIT",
  "engines": {
    "atom": ">= 1.28"
  },
  "prettier": {
    "printWidth": 100
  },
  "dependencies": {
    "ansi-to-html": "^0.7.2",
    "atom-notify": "^1.1.0",
    "atom-select-list": "^0.8.1",
    "atom-space-pen-views": "^2.2.0",
    "classnames": "^2.5.1",
    "cross-env": "7.0.3",
    "fs-plus": "^2.10.1",
    "fuzzaldrin": "^2.1.0",
    "linkify-urls": "^2.2.0",
    "node-emoji": "^1.11.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "typescript": "^5.9.3",
    "underscore-plus": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@parcel/transformer-babel": "^2.16.4",
    "@parcel/transformer-coffeescript": "^2.16.4",
    "@parcel/transformer-typescript-tsc": "^2.16.4",
    "@types/atom": "^1.40.18",
    "@types/mocha": "^8",
    "@types/react": "^16.14.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "coffeescript": "^2.7.0",
    "eslint": "^9.39.2",
    "eslint-plugin-jasmine": "^4.2.2",
    "eslint-plugin-react": "^7.37.5",
    "lint": "^1.1.2",
    "parcel": "^2.16.4",
    "prettier": "^2.3.2",
    "quibble": "^0.3.0"
  },
  "targets": {
    "bundle": {
      "context": "electron-renderer",
      "engines": {
        "electron": ">=12.x"
      },
      "includeNodeModules": {
        "atom": false,
        "electron": false,
        "typescript": false
      },
      "outputFormat": "commonjs",
      "isLibrary": true
    }
  }
}
